    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug(gdb)",
            "type": "cppdbg",
            "request": "launch",
            "program": "/tmp/buildtempc",//将产生的可执行文件放入缓存文件夹
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/Resources",//定位到资源目录
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build(gcc)",
            "miDebuggerPath": "/usr/bin/gdb",
        },
    ]
}
